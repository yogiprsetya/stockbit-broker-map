{
  "manifest_version": 3,
  "name": "Stockbit Broker Map",
  "version": "1.0",
  "description": "Mengganti kode broker dengan tipe (Whale, S-Money, Retail, Mix) di Stockbit",
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "*://*.stockbit.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "sc.css"
      ],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}